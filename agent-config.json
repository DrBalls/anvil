{
  "agents": {
    "orchestrator": {
      "enabled": true,
      "model": "claude-3-opus",
      "maxConcurrency": 3,
      "timeout": 300000,
      "retryAttempts": 2
    },
    "requirements-analyzer": {
      "enabled": true,
      "autoValidate": true,
      "strictMode": false,
      "capabilities": [
        "analyze-capability",
        "analyze-enabler",
        "validate-requirements",
        "check-dependencies",
        "suggest-improvements"
      ]
    },
    "design-architect": {
      "enabled": true,
      "capabilities": [
        "create-system-design",
        "create-api-design",
        "create-database-schema",
        "generate-diagrams",
        "design-components"
      ],
      "diagramTypes": ["mermaid", "plantuml", "draw.io"]
    },
    "code-generator": {
      "enabled": true,
      "languages": ["javascript", "typescript"],
      "frameworks": {
        "backend": ["node", "express", "fastify"],
        "frontend": ["react", "vue", "angular"],
        "database": ["mongodb", "postgresql", "mysql"]
      },
      "capabilities": [
        "generate-backend",
        "generate-frontend",
        "generate-api",
        "generate-scaffolding",
        "generate-crud"
      ]
    },
    "test-automator": {
      "enabled": true,
      "coverageThreshold": 80,
      "frameworks": {
        "unit": ["jest", "mocha", "vitest"],
        "integration": ["supertest", "chai"],
        "e2e": ["cypress", "playwright", "puppeteer"]
      },
      "capabilities": [
        "generate-unit-tests",
        "generate-integration-tests",
        "generate-e2e-tests",
        "validate-requirements",
        "analyze-coverage"
      ]
    },
    "documentation-generator": {
      "enabled": true,
      "formats": ["markdown", "html", "pdf"],
      "capabilities": [
        "generate-api-docs",
        "generate-user-guide",
        "generate-readme",
        "generate-inline-docs",
        "generate-adr"
      ],
      "templates": {
        "api": "openapi",
        "readme": "standard",
        "guide": "comprehensive"
      }
    }
  },
  "workflows": {
    "full-implementation": {
      "name": "Full Implementation Pipeline",
      "enabled": true,
      "requireApproval": true,
      "stages": [
        {
          "name": "analyze",
          "agent": "requirements-analyzer",
          "action": "analyze",
          "required": true
        },
        {
          "name": "design",
          "agent": "design-architect",
          "action": "createDesign",
          "required": true,
          "approvalRequired": true
        },
        {
          "name": "generate",
          "agent": "code-generator",
          "action": "generate",
          "required": true
        },
        {
          "name": "test",
          "agent": "test-automator",
          "action": "generateTests",
          "required": true
        },
        {
          "name": "document",
          "agent": "documentation-generator",
          "action": "generateDocs",
          "required": false
        }
      ]
    },
    "quick-analysis": {
      "name": "Quick Analysis",
      "enabled": true,
      "requireApproval": false,
      "stages": [
        {
          "name": "analyze",
          "agent": "requirements-analyzer",
          "action": "quickAnalysis",
          "required": true
        }
      ]
    },
    "design-only": {
      "name": "Design Only",
      "enabled": true,
      "requireApproval": true,
      "stages": [
        {
          "name": "analyze",
          "agent": "requirements-analyzer",
          "action": "analyze",
          "required": true
        },
        {
          "name": "design",
          "agent": "design-architect",
          "action": "createFullDesign",
          "required": true
        }
      ]
    },
    "test-generation": {
      "name": "Test Generation",
      "enabled": true,
      "requireApproval": false,
      "stages": [
        {
          "name": "analyze-code",
          "agent": "test-automator",
          "action": "analyzeCode",
          "required": true
        },
        {
          "name": "generate-tests",
          "agent": "test-automator",
          "action": "generateAllTests",
          "required": true
        }
      ]
    }
  },
  "integrations": {
    "claudeCode": {
      "enabled": true,
      "apiEndpoint": "claude-code-api",
      "modelVersion": "claude-3-opus-20240229"
    },
    "github": {
      "enabled": false,
      "autoCommit": false,
      "branchStrategy": "feature-branch"
    },
    "slack": {
      "enabled": false,
      "notifications": ["workflow-complete", "workflow-failed"]
    }
  },
  "settings": {
    "autoSave": true,
    "logLevel": "info",
    "cacheResults": true,
    "cacheDuration": 3600000,
    "maxHistorySize": 100,
    "enableMetrics": true
  }
}